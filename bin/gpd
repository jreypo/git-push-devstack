#!/usr/bin/env bash

set -e

my_dir="$( cd "$( dirname "${BASH_SOURCE[0]}" )" && pwd )"

my_name=$(basename "$0")
USAGE="Usage: $my_name vm [<options>]
   or: $my_name laptop [<options>]"

forward=gpd-$1
if [[ -x "$my_dir/$forward" ]]; then
    "$my_dir/$forward" "${@:2}"
else
    printf "$USAGE\n"
    exit 1
fi

